<%@ taglib prefix="s" uri="/struts-tags"%>
<%@ taglib prefix="sj" uri="/struts-jquery-tags"%>
<%@page session="true"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8" />
<s:actionerror />
<s:fielderror />
<link rel="stylesheet" href="style/login.css" rel="stylesheet" type="text/css" />
<link rel="stylesheet" href="style/common.css" rel="stylesheet" type="text/css"/>
<script type="text/javascript" src="js/login.js"></script>
<title>application.login</title>
</head>
<body>
	<s:form namespace="/" action="login" method="post">
			<s:textfield key="userid" cssClass="userid"/>
